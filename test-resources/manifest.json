{
    "commerceSuiteVersion": "2011",
    "useCloudExtensionPack": true,
    "enableImageProcessingService": true,
    "extensions": [
      "cloudmediaconversion",
      "mediaconversionbackoffice",
      "cmsoccaddon"
    ],
    "useConfig": {
      "properties": [
        {
          "location": "hybris/config/local.properties"
        }
      ],
      "extensions": {
        "location": "test-resources/localextensions.xml",
        "exclude": [
          "mediaconversion",
          "mediaconversionbackoffice"
        ]
      }
    },
    "properties": [
        {
            "key": "any.key",
            "value": "any value",
            "persona": "development",
            "secret": true
        },
        {
            "key": "any.key",
            "value": "any value",
            "persona": "staging",
            "secret": true
        },
        {
            "key": "any.key",
            "value": "any value",
            "persona": "production",
            "secret": true
        },
        {
            "key": "any.key",
            "value": "any value",
            "persona": "local-dev",
            "secret": true
        }
    ],
    "storefrontAddons": [
      {
        "addon": "smarteditaddon",
        "storefront": "mestorefront",
